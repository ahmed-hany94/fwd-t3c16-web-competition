    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.20",
    "pg": "^8.5.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
